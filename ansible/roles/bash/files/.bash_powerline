# load powerline
if [ -f $(which powerline-daemon) ]; then
  powerline-daemon -q
  POWERLINE_BASH_CONTINUATION=1
  POWERLINE_BASH_SELECT=1
fi
if [ -f /usr/local/lib/python*/dist-packages/powerline/bindings/bash/powerline.sh ]; then
  source /usr/local/lib/python*/dist-packages/powerline/bindings/bash/powerline.sh
fi
